% layout 'common';
  <div class="page_header">
    <a href="/">Homes</a>
  </div>
  <table class="project_list">
    <tr>
      <th>Homes</th>
    </tr>
    % for my $home (@$homes) {
      <tr class="dark">
        <td>
          <a class="list" href="<%= url_for('project', home => gitweblite_rel($home)); %>">
            <%= $home %>
          </a>
        </td>
    % }
  </table>
