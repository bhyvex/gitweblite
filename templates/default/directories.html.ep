% layout 'common';
  <div class="page_header">
    <a href="/">Project directories</a>
  </div>
  <table class="project_list">
    <tr>
      <th>Project directories</th>
    </tr>
    % for my $directory (@$directories) {
      <tr class="dark">
        <td>
          <a class="list" href="<%= url_for('/projects')->query(project_dir => $directory) %>">
            <%= $directory %>
          </a>
        </td>
    % }
  </table>
